#: kfmclient.cpp:55
msgid "kfmclient"
msgstr "kfmclient"

#: kfmclient.cpp:56
msgid "KDE tool for opening URLs from the command line"
msgstr "आदेश रेखाबाट यूआरएल खोल्नका लागि केडीई उपकरण"

#: kfmclient.cpp:74
msgid "Non interactive use: no message boxes"
msgstr "अन्तरक्रियात्मक नभएको प्रयोग: कुनै सन्देश बाकस छैन"

#: kfmclient.cpp:76
msgid "Show available commands"
msgstr "उपलब्ध आदेश देखाउनुहोस्"

#: kfmclient.cpp:78
msgid "Command (see --commands)"
msgstr "आदेश (आदेश -- हेर्नुहोस्)"

#: kfmclient.cpp:80
msgid "Arguments for command"
msgstr "आदेशका लागि तर्क"

#: kfmclient.cpp:90
msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"वाक्य संरचना:\n"

#: kfmclient.cpp:91
msgid ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Opens a window showing 'url'.\n"
"            #  'url' may be a relative path\n"
"            #   or file name, such as . or subdir/\n"
"            #   If 'url' is omitted, $HOME is used instead.\n"
"\n"
msgstr ""
"  kfmclient openURL 'url' ['माइमप्रकार']\n"
"            # 'url' देखाउने सञ्झ्याल खोल्दछ ।\n"
"            #  'url' सापेक्षिक मार्ग हुन सक्छ\n"
"            #   वा फाइल नाम, जस्तै . वा subdir/\n"
"            #   यदि 'url' हटाइयो भने, सट्टामा $HOME प्रयोग हुन्छ ।\n"
"\n"

#: kfmclient.cpp:96
msgid ""
"            # If 'mimetype' is specified, it will be used to determine the\n"
"            #   component that Konqueror should use. For instance, set it "
"to\n"
"            #   text/html for a web page, to make it appear faster\n"
"\n"
msgstr ""
"            # यदि 'माइमप्रकार' निर्दिष्ट छ भने, कन्क्वेररले प्रयोग गरेको "
"अवयव निर्धारण गर्न\n"
"            #   यो प्रयोग गरिनेछ । उदाहरणका लागि, यसलाई चाँडो देखाउन, वेब "
"पृष्ठका लागि\n"
"            #   text/html मा सेट गर्नुहोस्\n"
"\n"

#: kfmclient.cpp:100
msgid ""
"  kfmclient newTab 'url' ['mimetype']\n"
"            # Same as above but opens a new tab with 'url' in an existing "
"Konqueror\n"
"            #   window on the current active desktop if possible.\n"
"\n"
msgstr ""
"  kfmclient newTab 'url' ['माइमप्रकार']\n"
"            # माथि दिए जस्तै तर एउटा अवस्थित कन्क्वेररमा 'url' सँगै नयाँ "
"ट्याब खोल्दछ\n"
"            #   यदि सम्भव भए हालको सक्रिय डेस्कटपमा सञ्झ्याल ।\n"
"\n"

#: kfmclient.cpp:104
msgid ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # Opens a window using the given profile.\n"
"            #   'profile' is a file under "
"~/.kde/share/apps/konqueror/profiles.\n"
"            #   'url' is an optional URL to open.\n"
"\n"
msgstr ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # दिएको प्रोफाइल प्रयोग गरेर सञ्झ्याल खोल्दछ ।\n"
"            #   'प्रोफाइल' ~/.kde/share/apps/konqueror/profiles अन्तर्गतको "
"फाइल हो ।\n"
"            #   'url' खोल्नका लागि एउटा वैकल्पिक यूआरएल हो ।\n"
"\n"

#: kfmclient.cpp:450
#, kde-format
msgid "Profile %1 not found\n"
msgstr "%1 प्रोफाइल फेला परेन\n"

#: kfmclient.cpp:481
msgid "Syntax Error: Not enough arguments\n"
msgstr "वाक्य संरचना त्रुटि: पर्याप्त तर्क छैन\n"

#: kfmclient.cpp:486
msgid "Syntax Error: Too many arguments\n"
msgstr "वाक्य संरचना त्रुटि: अति धेरै तर्क\n"

#: kfmclient.cpp:553
#, kde-format
msgid "Syntax Error: Unknown command '%1'\n"
msgstr "वाक्य संरचना त्रुटि: अज्ञात आदेश '%1'\n"
